{
  "name": "my-cv",
  "version": "1.0.0",
  "description": "My CV, made with web technologies",
  "license": "MIT",
  "scripts": {
    "build": "node build.js",
    "dev": "browser-sync start --server dist --files 'dist/**/*' --no-notify --no-open",
    "watch": "chokidar 'src/**/*' -c 'yarn build'",
    "start": "concurrently 'yarn watch' 'yarn dev'",
    "print": "yarn build && ./print.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "browser-sync": "^3.0.4",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "3"
  }
}
